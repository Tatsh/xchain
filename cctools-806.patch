+#ifndef	_MACH_I386_THREAD_STATUS_H_
+#define _MACH_I386_THREAD_STATUS_H_
+
+#include <mach/i386/_structs.h>
+#include <mach/message.h>
+#include <mach/i386/fp_reg.h>
+#include <mach/i386/thread_state.h>
