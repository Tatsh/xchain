+	fldmiaxeq	r7, {d1}
+	fldmfdxeq	r8, {d2}
+	fldmiaxeq	r9!, {d3}
+	fldmfdxeq	r10!, {d4}
+	fldmdbxeq	r11!, {d5}
+	fldmeaxeq	r12!, {d6}
+
+	fstmiaseq	r13, {s2}
+	fstmeaseq	r14, {s1}
+	fstmiaseq	r1!, {s31}
+	fstmeaseq	r2!, {s30}
+	fstmdbseq	r3!, {s29}
+	fstmfdseq	r4!, {s28}
+
+	fstmiaxeq	r5, {d7}
+	fstmeaxeq	r6, {d8}
+	fstmiaxeq	r7!, {d9}
+	fstmeaxeq	r8!, {d10}
+	fstmdbxeq	r9!, {d11}
+	fstmfdxeq	r10!, {d12}
+
+	@ Conversion operations
+
+	fsitoseq	s27, s6
+	ftosiseq	s25, s5
+	ftosizseq	s23, s4
+	ftouiseq	s21, s3
+	ftouizseq	s19, s2
+	fuitoseq	s17, s1
+
+	@ ARM from VFP operations
+
+	fmrseq	r11, s3
+	fmrxeq	r9, fpsid
+
+	@ VFP From ARM operations
+
+	fmsreq	s3, r9
+	fmxreq	fpsid, r8
+
