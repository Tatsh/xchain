+	       [ "$(RC_FORCEHDRS)" != "YES" ];				\
+	    then							\
+	    	echo === cctools does not install headers for macos ===;\
+	    else							\
+		(cd include; $(MAKE) DSTROOT=$(DSTROOT)			\
+			RC_OS="$(RC_OS)" install) || exit 1;		\
+	    fi;								\
+	else								\
+	    (cd include; $(MAKE) DSTROOT=$(DSTROOT) RC_OS=$(RC_OS) 	\
+		install) || exit 1;					\
+	fi
+
+$(DSTROOT):
+	$(MKDIRS) $@
+
+SRCROOT:
+	@if [ -n "${$@}" ]; \
+	then \
+		exit 0; \
+	else \
+		echo Must define $@; \
+		exit 1; \
+	fi
