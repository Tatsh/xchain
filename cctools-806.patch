+	fdivseq	s3, s2, s1
+	fmacseq	s31, s30, s29
+	fmscseq	s28, s27, s26
+	fmulseq	s25, s24, s23
+	fnmacseq	s22, s21, s20
+	fnmscseq	s19, s18, s17
+	fnmulseq	s16, s15, s14
+	fsubseq	s13, s12, s11
+
+	@ Load/store operations
+
+	fldseq	s10, [r8]
+	fstseq	s9, [r7]
+
+	@ Load/store multiple operations
+
+	fldmiaseq	r1, {s8}
+	fldmfdseq	r2, {s7}
+	fldmiaseq	r3!, {s6}
+	fldmfdseq	r4!, {s5}
+	fldmdbseq	r5!, {s4}
+	fldmeaseq	r6!, {s3}
