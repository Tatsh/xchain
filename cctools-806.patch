+		DSTROOT=$$DSTROOT $@) || exit 1;			\
+	    echo =========== $(MAKE) $@ for misc =============;		\
+	    (cd misc; $(MAKE) "RC_CFLAGS=$(RC_CFLAGS)"			\
+		RC_ARCHS="$(RC_ARCHS)" RC_OS="$(RC_OS)"			\
+		TRIE="$(TRIE)" LTO="$(LTO)"				\
+		DSTROOT=$$DSTROOT $@) || exit 1;			\
+	    (cd cbtlibs; $(MAKE) "RC_CFLAGS=$(RC_CFLAGS)"		\
+		RC_ARCHS="$(RC_ARCHS)" RC_OS="$(RC_OS)"			\
+		DSTROOT=$$DSTROOT $@) || exit 1;			\
+	fi
