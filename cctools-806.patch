@@ -0,0 +1,111 @@
+export USE_APPLE_PB_SUPPORT = all
+OFLAG = -Os
+RC_OS = macos
+X_CFLAGS =
+ifneq "" "$(SDKROOT)"
+  SDK = -isysroot $(SDKROOT)
