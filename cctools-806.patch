+	then								\
+	    CWD=`pwd`; cd "$(DSTROOT)"; DSTROOT=`pwd`; cd "$$CWD";	\
+	    SED_RC_CFLAGS=`echo "$(RC_CFLAGS)" | sed 's/-arch ppc64//'  \
+ 		| sed 's/-arch x86_64//'`;				\
+	    echo =========== $(MAKE) $@ for libstuff =============;	\
+	    (cd libstuff; $(MAKE) "RC_CFLAGS=$(RC_CFLAGS)"		\
