+
+#define VM_REGION_BASIC_INFO_64		9
+struct vm_region_basic_info_64 {
+	vm_prot_t		protection;
+	vm_prot_t		max_protection;
+	vm_inherit_t		inheritance;
+	boolean_t		shared;
+	boolean_t		reserved;
+	memory_object_offset_t	offset;
